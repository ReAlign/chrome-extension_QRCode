{
   "name": "QRCode",
   "description": "QRCode",
   "version": "0.0.1",
   "manifest_version": 2,
   "update_url": "https://clients2.google.com/service/update2/crx",
   "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
         "19": "img/128.png",
         "38": "img/128.png"
      }
   },
   "icons": {
      "16": "img/128.png",
      "48": "img/128.png",
      "128": "img/128.png"
   },
   "background": {
      "scripts": [
         "js/background.js"
      ]
   },
   "permissions": [
      "contextMenus",
      "tabs",
      "activeTab",
      "http://*/*",
      "https://*/*",
      "ftp://*/",
      "file://*/",
      "clipboardRead",
      "clipboardWrite",
      "notifications"
   ]
}